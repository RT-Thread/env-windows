//include "Consts.fmi"
const ConEmu   = 0x43454D55

macro Descr="ConEmu: Test GuiMacro" Key="AltShiftF12"
{{
  F11
  $if (menu.select("ConEmu",2) > 0)
    Enter ~
  $else
    MsgBox("Error","ConEmu plugin was not found in the list")
  $end
}}
